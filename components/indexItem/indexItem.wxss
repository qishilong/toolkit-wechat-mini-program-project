.van-button-wrapper{
    text-align: center;
    margin-bottom: 40rpx;
    position: relative;
}

.icon-style{
    font-size: 35rpx;
    position: absolute;
    right: 50rpx;
}

.van-button--large {
    width: 70% !important;
    height: 100rpx !important;
    font-size: 35rpx;
    color: var(--button-color--) !important;
    border: 1px solid var(--button-color--) !important;
}